{
    "name": "tanerincode/modulity",
    "version": "1.0.0",
    "description": "A Module Generator CLI Package",
    "license": "MIT",
    "keywords": [
        "laravel",
        "artisan",
        "module",
        "generator"
    ],
    "authors": [
        {
            "name": "Taner Tombas",
            "email": "tombastaner@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-mbstring": "*",
        "illuminate/console": ">=5.3.0",
        "illuminate/support": ">=5.3.0"
    },
    "autoload": {
        "psr-4": {
            "TanerInCode\\Modulity\\": "src/",
            "Modules\\" : "app/Modules/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TanerInCode\\Modulity\\ModulityServiceProvider"
            ]
        }
    }
}